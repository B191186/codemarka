{
  "hosting": {
    "public": "build/",
    "site": "codemarka",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "headers":[{
      "source":"**/*.@(jpg|jpeg|gif|svg|png)",
      "headers":[{
        "key":"Cache-control",
        "value":"max-age=300000"
      }]
    }]
  }
}